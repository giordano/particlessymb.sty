\NewParticleResonance{\PGSppcP}[1]{\PGSc}{#1}{}{++}
\NewParticleResonance{\PGSpcP}[1]{\PGSc}{#1}{}{+}
\NewParticleResonance{\PGSzcP}[1]{\PGSc}{#1}{}{0}
\NewParticle{\PGSstc}{\PGSc}{}{\ast}
\NewParticle{\PGSstppc}{\PGSstc}{}{++}
\NewParticle{\PGSstpc}{\PGSstc}{}{+}
\NewParticle{\PGSstzc}{\PGSstc}{}{0}
\NewParticleResonance{\PGSstcP}[1]{\PGSstc}{#1}{}{}
\NewParticleResonance{\PGSstppcP}[1]{\PGSstc}{#1}{}{++}
\NewParticleResonance{\PGSstpcP}[1]{\PGSstc}{#1}{}{+}
\NewParticleResonance{\PGSstzcP}[1]{\PGSstc}{#1}{}{0}
%% Sigma bottom
\NewParticle{\PGSb}{\PGS}{b}{}
\NewParticle{\PGSmb}{\PGS}{b}{-}
\NewParticle{\PGSpb}{\PGS}{b}{+}
\NewParticle{\PGSzb}{\PGS}{b}{0}
\NewParticle{\PGSstb}{\PGS}{b}{\ast}
\NewParticle{\PGSstmb}{\PGS}{b}{\ast{}-}
\NewParticle{\PGSstpb}{\PGS}{b}{\ast{}+}
\NewParticle{\PGSstzb}{\PGS}{b}{\ast{}0}

%% Xi
\NewParticle{\PGX}{\Upxi}{}{}
\NewAntiParticle{\PAGX}{\PGX}{}{}
%% Xi strange
\NewParticle{\PGXm}{\PGX}{}{-}
\NewParticle{\PGXz}{\PGX}{}{0}
\NewParticle{\PGXstm}{\PGX}{}{\ast-}
\NewParticle{\PGXstz}{\PGX}{}{\ast0}
\NewParticleResonanceFull{\PGXP}[1]{\PGX}{}{}{#1}{}{}
\NewParticleResonanceFull{\PGXmP}[1]{\PGX}{}{}{#1}{}{-}
\NewParticleResonanceFull{\PGXzP}[1]{\PGX}{}{}{#1}{}{0}
\NewParticleResonanceFull{\PGXstmP}[1]{\PGX}{}{\ast}{#1}{}{-}
\NewParticleResonanceFull{\PGXstzP}[1]{\PGX}{}{\ast}{#1}{}{0}

%% Xi charm
\NewParticle{\PGXc}{\PGX}{c}{}
\NewParticle{\PGXpc}{\PGX}{c}{+}
\NewParticle{\PGXzc}{\PGX}{c}{0}
\NewParticleResonanceFull{\PGXcP}[1]{\PGX}{c}{}{#1}{}{}
\NewParticle{\PGXprc}{\PGX}{c}{\prime{}}
\NewParticle{\PGXprpc}{\PGX}{c}{\prime{}+}
\NewParticle{\PGXprzc}{\PGX}{c}{\prime{}0}
\NewParticle{\PGXstc}{\PGX}{c}{\ast{}}
