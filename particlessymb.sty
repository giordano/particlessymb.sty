\let\Xspace\xspace

% commands for defining new particles and antiparticles
\DeclareDocumentCommand\NewParticle{ m m m m }%
{\DeclareRobustCommand{#1}{\HepParticle{#2}{#3}{#4}\Xspace}}
\DeclareDocumentCommand\NewAntiParticle{ m m m m }%
{\DeclareRobustCommand{#1}{\HepAntiParticle{#2}{#3}{#4}\Xspace}}

% commands for defining new particles with resonance
\DeclareDocumentCommand\NewParticleResonance{ m o m m m m }%
{\DeclareRobustCommand{#1}[#2]{\HepParticleResonance{#3}{#4}{#5}{#6}\Xspace}}
\DeclareDocumentCommand\NewParticleResonanceFull{ m o m m m m m m }%
{\DeclareRobustCommand{#1}[#2]{\HepParticleResonanceFull{#3}{#4}{#5}{#6}{#7}{#8}\Xspace}}

% commands for defining new SUSY particles and antiparticles
\DeclareDocumentCommand\NewSusyParticle{ m m m m }%
{\DeclareRobustCommand{#1}{\HepSusyParticle{#2}{#3}{#4}\Xspace}}
\DeclareDocumentCommand\NewSusyAntiParticle{ m m m m }%
{\DeclareRobustCommand{#1}{\HepSusyAntiParticle{#2}{#3}{#4}\Xspace}}

%% -------------------------------------------------------------
%% The particles...
%% -------------------------------------------------------------

% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
%             Standard model bosons and Higgs                    +
% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

%% W and Z bosons
\NewParticle{\Pg}{g}{}{} % gluon
\NewParticle{\PGg}{\upgamma}{}{} % photon

\NewParticle{\PW}{W}{}{} % W boson
\NewParticle{\PWpm}{\PW}{}{\pm} % charged W boson
\NewParticle{\PWmp}{\PW}{}{\mp} % charged W boson
\NewParticle{\PWp}{\PW}{}{+} % W-plus
\NewParticle{\PWm}{\PW}{}{-} % W-minus

\NewParticle{\PZ}{Z}{}{} % Z boson
\NewParticle{\PZz}{\PZ}{}{0} % neutral Z boson

%% Higgs
\NewParticle{\PH}{H}{}{} % standard/heavy Higgs
\NewParticle{\PHz}{\PH}{}{0} % explicitly neutral standard/heavy Higgs

% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
%            W and Z Bosons outside Standard Model               +
% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

\NewParticle{\PWDt}{\PW}{2}{}% W2
\NewParticle{\PWpDt}{\PW}{2}{+}% W2 +
\NewParticle{\PWpr}{\PW}{}{\prime} % W'
\NewParticle{\PWL}{\PW}{L}{}
\NewParticle{\PWR}{\PW}{R}{}

\NewParticle{\PZpr}{\PZ}{}{\prime} % Z'
\NewParticle{\PZprpr}{\PZ}{}{\prime\prime}% Z"
\NewParticle{\PZst}{\PZ}{}{\ast} % Z*
\NewParticle{\PZzDt}{\PZ}{2}{0} % Z-zero 2
\NewParticle{\PZzDT}{\PZ}{3}{0} % Z-zero 3
\NewParticle{\PZL}{\PZ}{L}{}
\NewParticle{\PZLR}{\PZ}{LR}{}
\NewParticle{\PZR}{\PZ}{R}{}
\NewParticle{\PZGc}{\PZ}{\upchi}{}
\NewParticle{\PZGe}{\PZ}{\upeta}{}
\NewParticle{\PZGy}{\PZ}{\uppsi}{}

% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
%                          LEPTONS                               +
% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

\NewParticle{\Pl}{l}{}{} % lepton
\NewParticle{\PlR}{\Pl}{R}{} % right-handed lepton
\NewParticle{\Plpm}{\Pl}{}{\pm} % charged lepton
\NewParticle{\Plp}{\Pl}{}{+} % positive lepton
\NewParticle{\Plm}{\Pl}{}{-} % negative lepton
\NewParticle{\PlmR}{\Pl}{R}{-} % right-handed negative lepton
\NewAntiParticle{\PAl}{\Pl}{}{} % anti-lepton
\NewParticle{\PGnl}{\PGn}{l}{} % neutrino (for lepton l)
\NewAntiParticle{\PAGnl}{\PGn}{l}{} % anti-neutrino (for lepton l)

\NewParticle{\Pe}{e}{}{} % electron
\NewParticle{\Pepm}{\Pe}{}{\pm} % e plus/minus
\NewParticle{\Pemp}{\Pe}{}{\mp} % e minus/plus
\NewParticle{\Pem}{\Pe}{}{-} % electron
\NewParticle{\Pep}{\Pe}{}{+} % positron
%%% For nuclear physics: beta decay
\NewParticle{\PGb}{\upbeta}{}{} % beta
\NewParticle{\PGbm}{\PGb}{}{-} % beta-
\NewParticle{\PGbp}{\PGb}{}{+} % beta+

\NewParticle{\PGm}{\upmu}{}{} % muon
\NewParticle{\PGmpm}{\PGm}{}{\pm} % mu plus/minus
\NewParticle{\PGmm}{\PGm}{}{-} % muon
\NewParticle{\PGmp}{\PGm}{}{+} % anti-muon

\NewParticle{\PGt}{\uptau}{}{} % tau
\NewParticle{\PGtpm}{\PGt}{}{\pm} % tau plus/minus
\NewParticle{\PGtm}{\PGt}{}{-} % tau lepton
\NewParticle{\PGtp}{\PGt}{}{+} % anti-tau

\NewParticle{\PGtpr}{\PGt}{}{\prime} % tau prime
\NewParticle{\PGtprm}{\PGtpr}{}{-} % tau prime lepton
\NewParticle{\PGtprp}{\PGtpr}{}{+} % anti-tau prime

\NewParticle{\PGn}{\upnu}{}{} % neutrino
\NewParticle{\PGne}{\PGn}{\!\Pe}{} % electron neutrino
\NewParticle{\PGnGm}{\PGn}{\!\PGm}{} % muon neutrino
\NewParticle{\PGnGt}{\PGn}{\!\PGt}{} % tau neutrino
\NewParticle{\PGnGtpr}{\PGn}{\!\PGtpr}{} % tau prime neutrino

\NewAntiParticle{\PAGn}{\PGn}{}{} % anti-neutrino
\NewAntiParticle{\PAGne}{\PGn}{\!\Pe}{} % electron anti-neutrino
\NewAntiParticle{\PAGnGm}{\PGn}{\!\PGm}{} % muon anti-neutrino
\NewAntiParticle{\PAGnGt}{\PGn}{\!\PGt}{} % tau anti-neutrino
\NewAntiParticle{\PAGnGtpr}{\PGn}{\!\PGtpr}{} % tau prime anti-neutrino

%%% Heavy leptons
\NewParticle{\PL}{L}{}{}
\NewParticle{\PLm}{L}{}{\mspace{-1mu}-}
\NewParticle{\PLp}{L}{}{\mspace{-1mu}+}
\NewParticle{\PLz}{L}{}{\mspace{-1mu}0}
\NewParticle{\PLpm}{L}{}{\mspace{-1mu}\pm}
\NewAntiParticle{\PAL}{L}{}{}

% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
%                          QUARKS                                +
% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

\NewParticle{\PQq}{q}{}{} % quark (generic)
\NewParticle{\PQd}{d}{}{} % down quark
\NewParticle{\PQu}{u}{}{} % up quark
\NewParticle{\PQs}{s}{}{} % strange quark
\NewParticle{\PQc}{c}{}{} % charm quark
\NewParticle{\PQb}{b}{}{} % bottom quark
\NewParticle{\PQt}{t}{}{} % top quark
\NewParticle{\PQbpr}{\PQb}{}{\prime} % bottom prime quark
\NewParticle{\PQtpr}{\PQt}{}{\prime} % top prime quark

\NewAntiParticle{\PAQq}{\PQq}{}{} % anti-quark (generic)
\NewAntiParticle{\PAQd}{\PQd}{}{} % down anti-quark
\NewAntiParticle{\PAQu}{\PQu}{}{} % up anti-quark
\NewAntiParticle{\PAQs}{\PQs}{}{} % strange anti-quark
\NewAntiParticle{\PAQc}{\PQc}{}{} % charm anti-quark
\NewAntiParticle{\PAQb}{\PQb}{}{} % bottom anti-quark
\NewAntiParticle{\PAQt}{\PQt}{}{} % top anti-quark
\NewAntiParticle{\PAQbpr}{\PQb}{}{\prime} % bottom prime anti-quark
\NewAntiParticle{\PAQtpr}{\PQt}{}{\prime} % top prime anti-quark

% Versions generic quark with quark identifier as subscript
\NewParticle{\PQqb}{\PQq}{\PQb}{}
\NewParticle{\PQqbpr}{\PQq}{\PQbpr}{}
\NewParticle{\PQqc}{\PQq}{\PQc}{}
\NewParticle{\PQqd}{\PQq}{\PQd}{}
\NewParticle{\PQqdR}{\PQq}{{\PQd}R}{}
\NewParticle{\PQqs}{\PQq}{\PQs}{}
\NewParticle{\PQqt}{\PQq}{\PQt}{}
\NewParticle{\PQqtpr}{\PQq}{\PQtpr}{}
\NewParticle{\PQqu}{\PQq}{\PQu}{}
\NewParticle{\PQquR}{\PQq}{{\PQu}R}{}

\NewAntiParticle{\PAQqb}{\PQq}{\PQb}{}
\NewAntiParticle{\PAQqbpr}{\PQq}{\PQbpr}{}
\NewAntiParticle{\PAQqc}{\PQq}{\PQc}{}
\NewAntiParticle{\PAQqd}{\PQq}{\PQd}{}
\NewAntiParticle{\PAQqs}{\PQq}{\PQs}{}
\NewAntiParticle{\PAQqt}{\PQq}{\PQt}{}
\NewAntiParticle{\PAQqtpr}{\PQq}{\PQtpr}{}
\NewAntiParticle{\PAQqu}{\PQq}{\PQu}{}

% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
%                          BARYONS                               +
% ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++

% Nucleons
\NewParticle{\Pp}{p}{}{} % proton
\NewParticle{\Pn}{n}{}{} % neutron
\NewAntiParticle{\PAp}{\Pp}{}{} % anti-proton
\NewAntiParticle{\PAn}{\Pn}{}{} % anti-neutron
\NewParticle{\PGa}{\upalpha}{}{} % alpha

%% N resonances
\NewParticle{\PN}{N}{}{}
\NewParticle{\PNp}{\PN}{}{+}
\NewParticle{\PNz}{\PN}{}{0}
\NewParticleResonance{\PNP}[1]{\PN}{#1}{}{}
\NewParticleResonance{\PNpP}[1]{\PN}{#1}{}{+}
\NewParticleResonance{\PNzP}[1]{\PN}{#1}{}{-}

%% Delta baryons
\NewParticle{\PGD}{\Updelta}{}{}
\NewParticle{\PGDpp}{\PGD}{}{++}
\NewParticle{\PGDp}{\PGD}{}{+}
\NewParticle{\PGDz}{\PGD}{}{0}
\NewParticle{\PGDm}{\PGD}{}{-}
\NewParticleResonance{\PGDP}[1]{\PGD}{#1}{}{}
\NewParticleResonanceFull{\PGDppP}[1]{\PGD}{}{}{#1}{}{++}
\NewParticleResonanceFull{\PGDpP}[1]{\PGD}{}{}{#1}{}{+}
\NewParticleResonanceFull{\PGDzP}[1]{\PGD}{}{}{#1}{}{0}
\NewParticleResonanceFull{\PGDmP}[1]{\PGD}{}{}{#1}{}{-}

%% Theta and fi resonances
\NewParticleResonanceFull{\PGQpP}[1]{\Uptheta}{}{}{#1}{}{+}
\NewParticleResonanceFull{\PGFP}[1]{\Upphi}{}{}{#1}{}{}

%% Lambda strange
\NewParticle{\PGL}{\Uplambda}{}{} %% Lambda
\NewAntiParticle{\PAGL}{\PGL}{}{} %% Anti-lambda
\NewParticleResonance{\PGLP}[1]{\PGL}{#1}{}{}

%% Lambda charmed
\NewParticle{\PGLc}{\PGL}{c}{} %% Lambda_c
\NewParticle{\PGLpc}{\PGL}{c}{+} %% Lambda_c +
\NewParticleResonance{\PGLpcP}[1]{\PGLc}{#1}{}{+}

%% Lambda bottom
\NewParticle{\PGLb}{\PGL}{b}{}
\NewParticle{\PGLzb}{\PGL}{b}{0}

%% Sigma strange
\NewParticle{\PGS}{\Upsigma}{}{} %% Sigma
\NewAntiParticle{\PAGS}{\PGS}{}{}
\NewParticle{\PGSpm}{\PGS}{}{\pm} %% charged Sigma
\NewParticle{\PGSm}{\PGS}{}{-}
\NewParticle{\PGSp}{\PGS}{}{+}
\NewParticle{\PGSz}{\PGS}{}{0}
\NewParticle{\PGSstm}{\PGS}{}{\ast-}
\NewParticle{\PGSstp}{\PGS}{}{\ast+}
\NewParticle{\PGSstz}{\PGS}{}{\ast0}
\NewParticleResonance{\PGSP}[1]{\PGS}{#1}{}{}
\NewParticleResonanceFull{\PGSmP}[1]{\PGS}{}{}{#1}{}{-}
\NewParticleResonanceFull{\PGSpP}[1]{\PGS}{}{}{#1}{}{+}
\NewParticleResonanceFull{\PGSzP}[1]{\PGS}{}{}{#1}{}{0}
%% Sigma charm
